#include <iostream>
using namespace std;

int main()
{
	int value;
	cin >> value;
	unsigned int order = 32;
	unsigned int mask = 1 << order - 1;
	cout << value << "\n";
	for (int i = 1; i <= order; i++)
	{
		putchar(value & mask ? '1' : '0');
		value <<= 1;
		if (i % 8 == 0)
		{
			cout << " ";
		}
		if (i % order - 1 == 0)
		{
			cout << " ";
		}
	}
	return 0;
}
